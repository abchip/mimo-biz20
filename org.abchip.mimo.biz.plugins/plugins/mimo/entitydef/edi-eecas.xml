<?xml version="1.0" encoding="UTF-8"?>

<entity-eca
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="http://ofbiz.apache.org/dtds/entity-eca.xsd">

	<eca entity="EdiFrameSetup" operation="create" event="return">
		<action service="createEdiFrameSetup" mode="sync"
			value-attr="entityInstance" />
	</eca>
	<eca entity="EdiFrameSetup" operation="store" event="return">
		<action service="updateEdiFrameSetup" mode="sync"
			value-attr="entityInstance" />
	</eca>
	<eca entity="EdiFrameSetup" operation="remove" event="return">
		<action service="deleteEdiFrameSetup" mode="sync"
			value-attr="entityInstance" />
	</eca>
	
	<eca entity="MessageType" operation="create" event="return">
		<action service="createMessageType" mode="sync"
			value-attr="entityInstance" />
	</eca>
	<eca entity="MessageType" operation="store" event="return">
		<action service="updateMessageType" mode="sync"
			value-attr="entityInstance" />
	</eca>
	<eca entity="MessageType" operation="remove" event="return">
		<action service="deleteMessageType" mode="sync"
			value-attr="entityInstance" />
	</eca>	
</entity-eca>