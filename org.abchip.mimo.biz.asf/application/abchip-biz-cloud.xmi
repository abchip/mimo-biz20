<?xml version="1.0" encoding="UTF-8"?>
<mimo-app:Application
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:biz="http://www.abchip.org/mimo/biz"
    xmlns:mimo-app="http://www.abchip.org/mimo/application"
    xmlns:mimo-ctx="http://www.abchip.org/mimo/context"
    xmlns:mimo-resource="http://www.abchip.org/mimo/resource"
    xsi:schemaLocation="http://www.abchip.org/mimo/biz ../../org.abchip.mimo.biz.model/model/biz.ecore http://www.abchip.org/mimo/application ../../org.abchip.mimo.core/model/mimo.ecore#//application http://www.abchip.org/mimo/context ../../org.abchip.mimo.core/model/mimo.ecore#//context http://www.abchip.org/mimo/resource ../../org.abchip.mimo.core/model/mimo.ecore#//resource"
    plugin="org.abchip.mimo.biz.asf"
    name="ofbiz-application"
    text="OFBiz Application">
  <components
      name="Core">
    <commands
        className="org.abchip.mimo.core.base.BaseEntityCommandProviderImpl"/>
    <modules
        name="Base">
      <services
          xsi:type="mimo-app:ServiceExecutor"
          className="org.abchip.mimo.core.base.util.BaseStringsImpl"
          interfaceName="org.abchip.mimo.util.Strings"/>
      <services
          xsi:type="mimo-app:ServiceExecutor"
          className="org.abchip.mimo.core.base.util.BaseListsImpl"
          interfaceName="org.abchip.mimo.util.Lists"/>
      <services
          xsi:type="mimo-app:ServiceExecutor"
          className="org.abchip.mimo.core.base.util.BaseFilesImpl"
          interfaceName="org.abchip.mimo.util.Files"/>
      <services
          xsi:type="mimo-app:ServiceExecutor"
          className="org.abchip.mimo.core.base.util.BaseStreamsImpl"
          interfaceName="org.abchip.mimo.util.Streams"/>
      <services
          xsi:type="mimo-app:ServiceExecutor"
          className="org.abchip.mimo.core.base.util.BaseClassesImpl"
          interfaceName="org.abchip.mimo.util.Classes"/>
      <services
          xsi:type="mimo-app:ServiceExecutor"
          className="org.abchip.mimo.core.base.util.BaseThreadsImpl"
          interfaceName="org.abchip.mimo.util.Threads"/>
      <services
          xsi:type="mimo-app:ServiceExecutor"
          className="org.abchip.mimo.core.base.util.BaseThreadManagerImpl"
          interfaceName="org.abchip.mimo.util.ThreadManager"/>
      <services
          xsi:type="mimo-app:ServiceExecutor"
          className="org.abchip.mimo.core.base.util.BaseURIsImpl"
          interfaceName="org.abchip.mimo.util.URIs"/>
      <services
          xsi:type="mimo-app:ServiceExecutor"
          className="org.abchip.mimo.core.e4.E4LoggerImpl"
          interfaceName="org.abchip.mimo.context.Logger"/>
      <services
          xsi:type="mimo-app:ServiceExecutor"
          className="org.abchip.mimo.core.base.BaseRegistryFactoryImpl"
          interfaceName="org.abchip.mimo.context.RegistryFactory"/>
    </modules>
    <modules
        name="Resource">
      <services
          xsi:type="mimo-app:ServiceExecutor"
          className="org.abchip.mimo.core.base.BaseLockManagerImpl"
          interfaceName="org.abchip.mimo.context.LockManager"/>
      <services
          xsi:type="mimo-app:ServiceExecutor"
          className="org.abchip.mimo.core.base.BaseResourceManagerImpl"
          interfaceName="org.abchip.mimo.resource.ResourceManager"/>
      <services
          xsi:type="mimo-app:ServiceRegistry"
          className="org.abchip.mimo.core.base.BaseResourceProviderRegistryImpl"
          interfaceName="org.abchip.mimo.resource.ResourceProviderRegistry">
        <entries
            className="org.abchip.mimo.EMFResourceProviderImpl"
            interfaceName="org.abchip.mimo.resource.ResourceProvider"
            name="*EMF"
            vendor="ABChip"
            version="0.1.0"/>
        <entries
            className="org.abchip.mimo.core.base.nio.NIOResourceProviderImpl"
            interfaceName="org.abchip.mimo.resource.ResourceProvider"
            name="*NIO"
            vendor="ABChip"
            version="0.1.0"/>
      </services>
      <services
          xsi:type="mimo-app:ServiceExecutor"
          className="org.abchip.mimo.core.base.BaseResourceManagerFactoryImpl"
          status="STP"
          interfaceName="org.abchip.mimo.context.Factory"/>
    </modules>
  </components>
  <components
      xsi:type="biz:BizComponent"
      name="Framework"
      plugin="org.abchip.mimo.biz.framework"
      modulesDir="framework">
    <config>
      <entities
          xsi:type="mimo-ctx:ProviderConfig"
          url="https://localhost:8080/biz/service">
        <publicUser
            user="guest"
            password="ofbiz"/>
      </entities>
    </config>
    <hooks className="org.abchip.mimo.biz.plugins.application.BizApplicationComponentHook"/>
    <modules
        name="Commands">
      <services
          xsi:type="mimo-app:ServiceCommandProvider"
          className="org.abchip.mimo.biz.plugins.command.BizTestCommands"/>
      <services
          xsi:type="mimo-app:ServiceCommandProvider"
          className="org.abchip.mimo.biz.plugins.command.BizTenantCommands"/>
      <services
          xsi:type="mimo-app:ServiceCommandProvider"
          className="org.abchip.mimo.biz.plugins.command.BizSeedCommands"/>
    </modules>
    <modules
        name="Resources">
      <services
          xsi:type="mimo-app:ServiceRegistryEntry"
          className="org.abchip.mimo.biz.plugins.driver.OFBizResourceProviderImpl"
          interfaceName="org.abchip.mimo.resource.ResourceProvider"
          name="*OFBIZ"
          vendor="ABChip"
          version="0.1.0"/>
      <services
          xsi:type="mimo-app:ServiceRegistryEntry"
          className="org.abchip.mimo.core.http.HttpResourceProviderImpl"
          interfaceName="org.abchip.mimo.resource.ResourceProvider"
          name="*HTTP"
          vendor="ABChip"
          version="0.1.0"/>
    </modules>
    <bizModules
        name="base"/>
    <bizModules
        name="entity"/>
    <bizModules
        name="security"/>
    <bizModules
        name="datafile"/>
    <bizModules
        name="minilang"/>
    <bizModules
        name="common"/>
    <bizModules
        name="service"/>
    <bizModules
        name="catalina"
        status="STP"/>
    <bizModules
        name="entityext"/>
    <bizModules
        name="webapp"/>
  </components>
  <components
      xsi:type="biz:BizComponent"
      name="Applications"
      plugin="org.abchip.mimo.biz.applications"
      modulesDir="applications">
    <hooks className="org.abchip.mimo.biz.plugins.application.BizApplicationComponentHook"/>
    <modules
        name="Commands">
      <services
          xsi:type="mimo-app:ServiceCommandProvider"
          className="org.abchip.mimo.biz.base.command.PartyCommandProviderImpl"/>
      <services
          xsi:type="mimo-app:ServiceCommandProvider"
          className="org.abchip.mimo.biz.base.command.FiCCommandProviderImpl"/>
    </modules>
    <bizModules
        name="datamodel"/>
    <bizModules
        name="party"/>
    <bizModules
        name="securityext"/>
    <bizModules
        name="content"/>
    <bizModules
        name="workeffort"/>
    <bizModules
        name="product"/>
    <bizModules
        name="manufacturing"/>
    <bizModules
        name="accounting"/>
    <bizModules
        name="humanres"/>
    <bizModules
        name="order"/>
    <bizModules
        name="marketing"/>
    <bizModules
        name="commonext"/>
  </components>
  <components
      xsi:type="biz:BizComponent"
      name="Plugins"
      plugin="org.abchip.mimo.biz.plugins"
      modulesDir="plugins">
    <hooks className="org.abchip.mimo.biz.plugins.application.BizApplicationComponentHook"/>
    <bizModules
        name="mimo"/>
    <bizModules
        name="passport"/>
  </components>
  <components
      name="Connectors">
    <modules
        name="Auth">
      <services
          xsi:type="mimo-app:ServiceExecutor"
          className="org.abchip.mimo.biz.base.BizAuthenticationManagerImpl"
          interfaceName="org.abchip.mimo.context.AuthenticationManager"/>
    </modules>
    <modules
        name="Session">
      <services
          xsi:type="mimo-app:ServiceServlet"
          className="org.abchip.mimo.core.http.servlet.session.LoginServlet"
          alias="/biz/service/login"/>
      <services
          xsi:type="mimo-app:ServiceServlet"
          className="org.abchip.mimo.core.http.servlet.session.LogoutServlet"
          alias="/biz/service/logout"/>
      <services
          xsi:type="mimo-app:ServiceServlet"
          className="org.abchip.mimo.core.http.servlet.session.StatusServlet"
          alias="/biz/service/status"/>
      <services
          xsi:type="mimo-app:ServiceServlet"
          className="org.abchip.mimo.biz.plugins.servlet.CheckLoginServlet"
          alias="/biz/service/entityCheckLogin"/>
      <services
          xsi:type="mimo-app:ServiceServlet"
          className="org.abchip.mimo.biz.plugins.servlet.ExternalCredentialServlet"
          alias="/biz/service/entityExternalCredential"/>
    </modules>
    <modules
        name="Rest">
      <services
          xsi:type="mimo-app:ServiceServlet"
          className="org.abchip.mimo.core.http.servlet.LookupServlet"
          alias="/biz/service/lookup"/>
      <services
          xsi:type="mimo-app:ServiceServlet"
          className="org.abchip.mimo.core.http.servlet.FindServlet"
          alias="/biz/service/find"/>
      <services
          xsi:type="mimo-app:ServiceServlet"
          className="org.abchip.mimo.core.http.servlet.DeleteServlet"
          alias="/biz/service/delete"/>
      <services
          xsi:type="mimo-app:ServiceServlet"
          className="org.abchip.mimo.core.http.servlet.SaveServlet"
          alias="/biz/service/save"/>
    </modules>
  </components>
  <components
      name="Data Interchange"
      status="STP">
    <hooks className="org.abchip.mimo.biz.plugins.edi.EdiHook"
        status="STP"/>
    <commands
        className="org.abchip.mimo.edi.base.BaseEdiCommandProviderImpl"/>
    <modules
        name="Base">
      <services
          xsi:type="mimo-app:ServiceExecutor"
          className="org.abchip.mimo.edi.base.BaseEdiManagerImpl"
          interfaceName="org.abchip.mimo.edi.EdiManager"/>
    </modules>
  </components>
  <contextDescription
      id="abchip-biz"
      user="mimo"/>
  <hooks className="org.abchip.mimo.biz.plugins.application.BizApplicationHook"/>
  <paths data="{mimo.home}/data"
      logs="{mimo.home}/logs"
      work="{mimo.home}/work"/>
  <resourceMapping>
    <rules xsi:type="mimo-resource:ResourceMappingRuleByPackage"
        provider="*OFBIZ"
        package="biz"/>
    <rules xsi:type="mimo-resource:ResourceMappingRuleByPackage"
        provider="*OFBIZ"
        package="edi"/>
    <rules xsi:type="mimo-resource:ResourceMappingRuleByFrame"
        provider="*NIO"
        frame="EntityIdentifiable"/>
  </resourceMapping>
</mimo-app:Application>
