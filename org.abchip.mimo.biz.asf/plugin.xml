<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>

<!--
-->

<plugin>
   <extension
         id="mimo-biz-application"
         name="Application"
         point="org.eclipse.core.runtime.applications">
      <application
            cardinality="singleton-global"
            thread="main"
            visible="true">
         <run
               class="org.abchip.mimo.core.e4.E4EquinoxApplicationImpl">
         </run>
      </application>
   </extension>
   <extension
         id="mimo-biz"
         point="org.eclipse.core.runtime.products">
      <product
            application="org.abchip.mimo.biz.asf.mimo-biz-application"
            name="MIMO Biz Application">
         <property
               name="appName"
               value="MIMO Biz Application">
         </property>
      </product>
   </extension>
</plugin>
